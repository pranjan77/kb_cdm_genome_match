{
    "ver": "0.0.1",
    "authors": [
        "pranjan77"
    ],
    "contact": "https://kbase.us/contact-us/",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [ 
        {
            "id": "genomeset_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseSearch.GenomeSet" ]
            }
        },
        {
            "id": "run_gtdb",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values":["1"],
            "field_type" : "checkbox",
            "checkbox_options":{
                "checked_value": 1,
                "unchecked_value": 0
            }
        },
        {
            "id": "max_count",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values":["10"],
			"field_type": "text",
			"text_options": {
				"validate_as": "int"
			}
        },
        {
            "id": "max_level",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values":["genus"],
            "field_type" : "dropdown",
            "dropdown_options": {
					"options": [
						{
							"value": "genus",
							"display": "genus",
							"id": "genus",
							"ui-name": "genus"
						},
						{
							"value": "species",
							"display": "species",
							"id": "species",
							"ui-name": "species"
						}
					]
				}
        }
        
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name":"kb_cdm_genome_match",
            "method": "run_kb_cdm_genome_match",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },{
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "genomeset_ref",
                    "target_property": "genomeset_ref",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter": "run_gtdb",
                    "target_property": "run_gtdb"
                },{
                    "input_parameter": "max_count",
                    "target_property": "max_count"
                },{
                    "input_parameter": "max_level",
                    "target_property": "max_level"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
